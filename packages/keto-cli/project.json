{
  "name": "keto-cli",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/keto-cli/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc"
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs keto-cli {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [
        {
          "target": "build",
          "params": "forward"
        }
      ]
    }
  }
}
